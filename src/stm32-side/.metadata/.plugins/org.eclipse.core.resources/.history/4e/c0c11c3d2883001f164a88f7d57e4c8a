#ifndef INC_CONFIG_H_
#define INC_CONFIG_H_

#include "main.h"
#include "display.h"

extern uint8_t paddleMotorState;
extern uint8_t diskState[4];

staic void startUp();

static inline void rotateList(){
	diskState[2] = diskState[1];
	diskState[1] = diskState[0];
	diskState[0] = 0;
}

#endif /* INC_CONFIG_H_ */
